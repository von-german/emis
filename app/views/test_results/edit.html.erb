<% provide(:title, "Update Result") %>
<h1>Update Result</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@test_result) do |f| %>
    <%= render partial: "layouts/errors", locals: { object: @test_result } %>

      <%= f.label :test_type, "Test type" %>
      <%= f.text_field :test_type, class: 'form-control' %>

      <%= f.label :test_info, "Additional test information" %>
      <%= f.text_area :test_info, class: 'form-control' %>

      <%= f.submit "Update result", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
